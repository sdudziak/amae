{
  "title": "RelaxHub – Dark",
  "version": 2,
  "settings": {
    "color": {
      "palette": [
        { "slug": "bg", "name": "Tło ciemne", "color": "#151616" },
        { "slug": "text", "name": "Tekst jasny", "color": "#EAE5DF" },
        { "slug": "ink", "name": "Atrament jasny", "color": "#E1D7C4" },
        { "slug": "primary", "name": "Złoty akcent", "color": "#E0B24B" },
        { "slug": "sea", "name": "Morski jasny", "color": "#70D0CD" },
        { "slug": "card", "name": "Karta ciemna", "color": "#1E1F1F" },
        { "slug": "muted", "name": "Muted ciemny", "color": "#2B2C2C" }
      ]
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--bg)",
      "text": "var(--wp--preset--color--text)"
    },
    "elements": {
      "link": { "color": { "text": "var(--wp--preset--color--ink)" } },
      "button": {
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "#1D1B16"
        },
        "border": { "radius": "12px" }
      }
    },
    "blocks": {
      "core/group": { "background": { "color": "transparent" } },
      "core/quote": {
        "color": { "text": "var(--wp--preset--color--ink)" },
        "border": { "color": "var(--wp--preset--color--muted)" }
      },
      "core/separator": {
        "color": { "background": "var(--wp--preset--color--muted)" }
      }
    }
  }
}
